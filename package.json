{
  "private": true,
  "name": "hillcitymnag",
  "description": "Monorepo for https://www.hillcitymnag.church",
  "version": "2.0.0",
  "repository": "https://github.com/bmealhouse/hillcitymnag.git",
  "author": "Brent Mealhouse <bmealhouse@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "echo \"lint-staged\"",
      "pre-push": "echo \"xo && yarn test\""
    }
  },
  "lint-staged": {
    "*.js": [
      "xo",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "verbose": true
  },
  "prettier": {
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false
  },
  "renovate": {
    "automerge": true,
    "automergeType": "branch-push",
    "pinVersions": true,
    "schedule": [
      "every friday"
    ]
  },
  "xo": {
    "envs": [
      "jest"
    ],
    "ignores": [
      "original-www/static/**"
    ],
    "prettier": true,
    "extends": "xo-react",
    "parser": "babel-eslint",
    "rules": {
      "capitalized-comments": "off",
      "no-unused-vars": [
        "error",
        {
          "ignoreRestSiblings": true,
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "@reach/skip-nav": "0.7.0",
    "babel-plugin-styled-components": "1.10.7",
    "gatsby": "2.19.12",
    "gatsby-image": "2.2.40",
    "gatsby-plugin-google-analytics": "2.1.35",
    "gatsby-plugin-manifest": "2.2.41",
    "gatsby-plugin-offline": "3.0.34",
    "gatsby-plugin-react-helmet": "3.1.22",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-sitemap": "2.2.27",
    "gatsby-plugin-styled-components": "3.1.19",
    "gatsby-plugin-typography": "2.3.22",
    "gatsby-source-buzzsprout": "0.2.1",
    "gatsby-source-prismic": "2.2.0",
    "gatsby-transformer-sharp": "2.3.14",
    "prismic-dom": "2.1.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-is": "16.12.0",
    "react-typography": "0.16.19",
    "styled-components": "5.0.0",
    "typeface-didact-gothic": "0.0.72",
    "typeface-fira-sans": "0.0.75",
    "typeface-lora": "0.0.72",
    "typeface-roboto": "0.0.75",
    "typeface-roboto-slab": "1.1.3",
    "typeface-source-sans-pro": "0.0.75",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@types/react": "16.9.19",
    "babel-eslint": "10.0.3",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-xo-react": "0.22.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.2",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "4.2.1",
    "lint-staged": "10.0.7",
    "now": "16.7.3",
    "prettier": "1.19.1",
    "start-server-and-test": "1.10.8",
    "typescript": "3.7.5",
    "xo": "0.25.3"
  },
  "scripts": {
    "build": "gatsby build",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "prefetch-data": "cd www && yarn prefetch-data",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.js\"",
    "serve": "gatsby serve",
    "test": "echo todo"
  }
}
